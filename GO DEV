//launch.jsonというJSONファイル
{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}",
            "env": {},
            "args": [
                "-debug.v",
                "-debug.run",
                "TestSample"
            ],
            "showLog": true
        }
    ]
}

