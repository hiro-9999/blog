 echo 'export PATH="$HOME/.jenv/bin:$PATH"' >> ~/.zshrc
 echo 'eval "$(jenv init -)"' >> ~/.zshrc
 source ~/.zshrc
 jenv add $(/usr/libexec/java_home -v 1.8) 
  jenv versions
  jenv global 1.8
  jenv doctor
  java -version
  export PATH="$HOME/.jenv/bin:$PATH"
  eval "$(jenv init -)"
  jenv enable-plugin export
  echo $JAVA_HOME

